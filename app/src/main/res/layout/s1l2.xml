<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:theme="@style/cartoon_stage1_level2"
    android:buttonStyle="@style/cartoon_level_buttons">


    <Button
        android:id="@+id/S1L2_B1"
        android:layout_marginTop="75dp"
        app:layout_constraintEnd_toEndOf="@+id/S1L2_B5"
        app:layout_constraintStart_toStartOf="@+id/S1L2_B5"
        app:layout_constraintTop_toBottomOf="@+id/S1L2_B5" />

    <Button
        android:id="@+id/S1L2_B2"
        android:layout_marginTop="75dp"
        app:layout_constraintEnd_toEndOf="@+id/S1L2_B3"
        app:layout_constraintStart_toStartOf="@+id/S1L2_B3"
        app:layout_constraintTop_toBottomOf="@+id/S1L2_B3" />

    <Button
        android:id="@+id/S1L2_B3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25" />

    <Button
        android:id="@+id/S1L2_B4"
        android:layout_marginTop="75dp"
        app:layout_constraintEnd_toEndOf="@+id/S1L2_B7"
        app:layout_constraintStart_toStartOf="@+id/S1L2_B7"
        app:layout_constraintTop_toBottomOf="@+id/S1L2_B7" />

    <Button
        android:id="@+id/S1L2_B5"
        app:layout_constraintVertical_bias="0.01"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/S1L2_B6"
        android:layout_marginTop="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/S1L2_B4"
        app:layout_constraintStart_toStartOf="@+id/S1L2_B4"
        app:layout_constraintTop_toBottomOf="@+id/S1L2_B4" />

    <Button
        android:id="@+id/S1L2_B7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.01" />

    <Button
        android:id="@+id/S1L2_B8"
        android:layout_marginTop="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/S1L2_B1"
        app:layout_constraintStart_toStartOf="@+id/S1L2_B1"
        app:layout_constraintTop_toBottomOf="@+id/S1L2_B1" />
</android.support.constraint.ConstraintLayout>